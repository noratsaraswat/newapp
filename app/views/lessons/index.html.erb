<h1>Listing lesson names</h1>

<table>
  <tr>
    
    <th>Lessonname</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lessons.each do |lesson| %>
  <tr>
    
    <td><%= lesson.lessonName %></td>
    <td><%= link_to 'Show', lesson %></td>
    <td><%= link_to 'Edit', edit_lesson_path(lesson) %></td>
    <td><%= link_to 'Destroy', lesson, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />


<%#= link_to 'New Lesson', new_lesson_path %>
<%= link_button_to "Create/Edit Lesson", url_for(new_lesson_path)%> &nbsp; &nbsp;
<%= link_button_to "Lesson Details", url_for(:action=>"index",:controller=>"developers")%>
